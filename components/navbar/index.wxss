/* components/navbar/index./* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */
.nav-wrap {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 9999999;
  /*width: 100%;*/
  height: 34px;
  color: #000;
  background-color: #f8f8f8;
}

/* 标题 */
.nav-title {
  position: absolute;
  /*top: 0;*/
  left: 0;
  right: 0;
  bottom: 0;
  overflow: hidden;
  max-width: 200px;
  margin: auto;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #2c2b2b;
  /*font: 500 36rpx/80rpx;*/
  font-weight: 500;
  font-size: 18px;
  line-height: 34px; /*同.nav-wrap高度*/
}

.nav-capsule {
  z-index: 1;
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 70px;
  height: 100%;
  margin-left: 15px;
}
/*垂直分隔线*/
.navbar-v-line {
  width: 0.5px;
  height: 50%;
  background-color: #e5e5e5;
}

.back-pre,
.back-home {
  width: 18px;
  height: 18px;
  /*padding: 10rpx;*/
  /*margin-top: 4rpx;*/
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}
